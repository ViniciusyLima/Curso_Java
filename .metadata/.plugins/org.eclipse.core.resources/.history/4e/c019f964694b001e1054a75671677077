package aula88;

import java.util.ArrayList;
import java.util.List;

public class exer8 {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Alex");
		list.add("Joao");
		list.add("Junior");
		list.add(2, "Marco");
		
		System.out.println(list.size());
		
		list.remove("Junior");
		
		for (String x : list) {
			System.out.println(x);
		}
		

	}

}
